<?xml version="1.0" encoding="UTF-8"?><databaseChangeLog        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">    <changeSet id="create-category-table" author="hedza06" context="dev, prod">        <createTable tableName="category">            <column name="id" type="bigint" autoIncrement="true">                <constraints primaryKey="true" nullable="false" />            </column>            <column name="name" type="varchar(64)">                <constraints nullable="false" />            </column>            <column name="description" type="varchar(255)" />            <column name="created_at" type="datetime" defaultValueComputed="NOW()" />            <column name="is_active" type="bit(1)" defaultValueBoolean="true" />        </createTable>    </changeSet></databaseChangeLog>