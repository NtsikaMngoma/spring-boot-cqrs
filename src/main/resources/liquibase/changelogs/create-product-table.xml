<?xml version="1.0" encoding="UTF-8"?><databaseChangeLog        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">    <changeSet id="create-product-table" author="hedza06" context="dev, prod">        <createTable tableName="product">            <column name="id" type="bigint" autoIncrement="true">                <constraints primaryKey="true" nullable="false" />            </column>            <column name="catalog_name" type="varchar(128)">                <constraints nullable="false" />            </column>            <column name="price" type="decimal(15,2)">                <constraints nullable="false" />            </column>            <column name="category_id" type="bigint">                <constraints nullable="false" />            </column>            <column name="created_at" type="datetime" defaultValueComputed="NOW()" />            <column name="is_active" type="bit(1)" defaultValueBoolean="true" />        </createTable>    </changeSet></databaseChangeLog>